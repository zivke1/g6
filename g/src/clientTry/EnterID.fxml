<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="600.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clientTry.EnterIDController">
   <children>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="Enter ID:">
               <HBox.margin>
                  <Insets left="6.0" top="33.0" />
               </HBox.margin></Label>
            <TextField fx:id="id">
               <HBox.margin>
                  <Insets left="6.0" top="30.0" />
               </HBox.margin></TextField>
         </children>
      </HBox>
      <HBox alignment="BOTTOM_RIGHT" prefHeight="100.0" prefWidth="200.0">
         <VBox.margin>
            <Insets bottom="10.0" right="10.0" />
         </VBox.margin>
      </HBox>
      <AnchorPane prefHeight="340.0" prefWidth="500.0">
         <children>
            <Button layoutX="353.0" layoutY="311.0" mnemonicParsing="false" onMouseClicked="#UpdateEmail" text="Update email" />
            <Button layoutX="14.0" layoutY="311.0" mnemonicParsing="false" onMouseClicked="#ShowDetails" text="Show details" />
         </children>
      </AnchorPane>
   </children>
</VBox>
