<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane prefHeight="800.0" prefWidth="1280.0" styleClass="multi-bg-example" stylesheets="@application.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <bottom>
      <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" styleClass="Button" text="Back to order">
               <HBox.margin>
                  <Insets right="750.0" top="40.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" styleClass="Button" text="Enter waiting list for the original order">
               <HBox.margin>
                  <Insets top="40.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </bottom>
   <top>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="TOP_RIGHT" prefHeight="100.0" prefWidth="100.0">
               <children>
                  <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0" styleClass="Button" text="?" />
               </children>
               <VBox.margin>
                  <Insets right="10.0" />
               </VBox.margin>
            </HBox>
            <VBox prefWidth="1281.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="textA" text="Sorry! The chosen park is full at the desired date">
                     <font>
                        <Font name="System Bold" size="20.0" />
                     </font>
                  </Text>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="textA" text="The next dates are available for you:">
                     <font>
                        <Font name="System Bold" size="20.0" />
                     </font>
                  </Text>
               </children>
            </VBox>
         </children>
      </VBox>
   </top>
   <center>
      <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <TableView maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="400.0" prefWidth="300.0" styleClass="textA">
              <columns>
                <TableColumn prefWidth="75.0" text="Date" />
                <TableColumn prefWidth="75.0" text="Hour" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <Slider maxHeight="-Infinity" orientation="VERTICAL" prefHeight="405.0" value="100.0" />
         </children>
      </HBox>
   </center>
</BorderPane>
